import java.io.*;
import java.util.*;
class Flow004{
    public static void main(String args[] ) {
       Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
       
        while(n-- >=1)
        {
           int no=sc.nextInt();
           String np=Integer.toString(no);
           System.out.println((Character.getNumericValue(np.charAt(0)))+(Character.getNumericValue(np.charAt(np.length()-1))));
           
            
        }
    }
}


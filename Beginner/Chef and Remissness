import java.io.*;
import java.util.*;
class Remiss{
    public static void main(String args[] ) {
       Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
       
        while(n-- >=1)
        {
           int a=sc.nextInt();
           int b=sc.nextInt();
           System.out.println(Math.max(a,b)+" "+(a+b));
         
        }
    }
}


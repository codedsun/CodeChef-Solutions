import java.io.*;
import java.util.*;
class SNAPE {
    public static void main(String args[] ) {
       Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        while(n-- >=1)
        {
           int b=sc.nextInt();
           int ls=sc.nextInt();
           float min=(float)Math.sqrt(Math.abs((ls*ls)-(b*b)));
           float max=(float)Math.sqrt((ls*ls)+(b*b));
           System.out.println(min +" "+max);
            
        }
       }
    }

import java.io.*;
import java.util.*;
class FLOW013
{
    public static void main(String args[])
    {
         Scanner sc=new Scanner(System.in);
         int t=sc.nextInt();
         while(t-- >=1)
         {
             int a=sc.nextInt();
             int b=sc.nextInt();
             int c=sc.nextInt();
             int total=a+b+c;
             if(total==180)
             {
                 if(a!=0 && b!=0 && c!=0)
                 {
                      System.out.println("YES");
                    }
                    else
                    {
                        System.out.println("NO");
                    }
                }
                else
                {
                     System.out.println("NO");
                }
                 
             
        }
    }
}
    
     
            
